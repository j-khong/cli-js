{
   "name": "@jkhong/cli-js",
   "version": "1.1.0",
   "description": "",
   "main": "src/index.js",
   "types": "./types/index.d.ts",
   "scripts": {
      "test": "./node_modules/mocha/bin/mocha tests/tests.js",
      "format": "prettier --write \"src/**/*.js\"",
      "lint": "eslint src/**/*.js",
      "prepublishOnly": "npm run lint && npm test && npx tsc",
      "preversion": "npm run lint",
      "version": "npm run format && git add -A src",
      "postversion": "git push && git push --tags"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run format && npm run lint",
         "pre-push": "npm test"
      }
   },
   "author": "Julien Khong <julienkhong@yahoo.fr>",
   "license": "ISC",
   "homepage": "https://github.com/j-khong/cli-js#readme",
   "repository": {
      "type": "git",
      "url": "https://github.com/j-khong/cli-js.git"
   },
   "bugs": {
      "url": "https://github.com/j-khong/cli-js/issues"
   },
   "devDependencies": {
      "eslint": "^6.8.0",
      "husky": "^4.2.3",
      "mocha": "^5.1.0",
      "prettier": "^2.0.2",
      "typescript": "4.9.5"
   },
   "dependencies": {
      "@jkhong/devutils": "1.5.1",
      "colors": "1.3.3",
      "minimist": "1.2.8",
      "simpl-schema": "1.5.5"
   }
}